<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.a930scoutingapp2017.TeleopMenu"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="0dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/balls"
        app:layout_constraintRight_toRightOf="@+id/imageView18"
        android:layout_marginTop="27dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="27dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/submitGearsTime"
        android:layout_marginTop="76dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="76dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/subtractBallsTeleop" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/goToTeleop2"
        android:layout_marginEnd="46dp"
        app:layout_constraintRight_toRightOf="@+id/imageView18"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="53dp"
        app:layout_constraintTop_toTopOf="@+id/gearsTeleop" />

    <Button
        android:id="@+id/goToTeleop2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Teleop 2"
        tools:ignore="HardcodedText"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/balls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Balls"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="61dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gears"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Gears"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/gearsTeleop"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="27dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/ballsTeleop"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:text="# of Balls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.502" />

    <TextView
        android:id="@+id/gearsTeleop"
        android:layout_width="142dp"
        android:layout_height="41dp"
        android:text="# of Gears:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="98dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="98dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/addGearsTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/submitGearsTime"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/gearsTeleop" />

    <ImageButton
        android:id="@+id/subtractGearsTeleop"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="93dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView17"
        android:layout_marginEnd="112dp"
        app:layout_constraintRight_toRightOf="@+id/gearsTimer"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/gears"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="17dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView17" />

    <ImageButton
        android:id="@+id/addBallsTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="7dp"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/submitBallsTime"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/ballsTeleop" />

    <ImageButton
        android:id="@+id/subtractBallsTeleop"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/goToTeleop2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/subtractGearsTeleop"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/gearsTimer"
        android:layout_width="106dp"
        android:layout_height="0dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:cursorVisible="true"
        android:drawableTint="@android:color/holo_blue_dark"
        android:ems="10"
        android:hint="Cycle Time"
        android:inputType="number"
        android:textColorLink="@android:color/holo_blue_dark"
        android:textCursorDrawable="@android:color/holo_blue_dark"
        tools:cursorVisible="true"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="103dp"
        app:layout_constraintBottom_toBottomOf="@+id/gears"
        android:layout_marginEnd="106dp"
        app:layout_constraintRight_toRightOf="@+id/addGearsTeleop"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/gears"
        app:layout_constraintTop_toTopOf="@+id/submitGearsTime" />

    <EditText
        android:id="@+id/ballsTimer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:cursorVisible="true"
        android:drawableTint="@android:color/holo_blue_dark"
        android:ems="10"
        android:hint="Cycle Time"
        android:inputType="number"
        android:textColorLink="@android:color/holo_blue_dark"
        android:textCursorDrawable="@android:color/holo_blue_dark"
        tools:cursorVisible="true"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractBallsTeleop"
        app:layout_constraintRight_toRightOf="@+id/gearsTimer"
        android:layout_marginTop="3dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="3dp"
        app:layout_constraintLeft_toLeftOf="@+id/ballsTeleop"
        app:layout_constraintTop_toTopOf="@+id/submitBallsTime" />

    <Button
        android:id="@+id/submitGearsTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit Time"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginEnd="20dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="151dp" />

    <Button
        android:id="@+id/submitBallsTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit Time"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginStart="11dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/ballsTeleop"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="25dp"
        app:layout_constraintLeft_toRightOf="@+id/ballsTimer" />

    <RadioGroup
        android:id="@+id/ballGroupTeleop"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/ballsTimer"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView16"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="21dp"
        app:layout_constraintLeft_toLeftOf="@+id/ballsTimer">

        <RadioButton
            android:id="@+id/noAttemptBallsTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="No Attempt"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/lowGoalTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Low"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/highGoalTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="High"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />
    </RadioGroup>

</android.support.constraint.ConstraintLayout>

