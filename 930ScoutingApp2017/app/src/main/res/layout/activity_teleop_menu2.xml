<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.a930scoutingapp2017.TeleopMenu2"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/goToPostMatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Post-Match"
        tools:ignore="HardcodedText"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Rope"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="67dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="0dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/goToPostMatch"
        app:layout_constraintRight_toRightOf="@+id/imageView21"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/subtractRoboErrorsTeleop"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="11dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractFoulsTeleop"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginBottom="20dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/climbYesNo"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="35dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/subtractRoboErrorsTeleop"
        android:layout_marginBottom="173dp" />

    <TextView
        android:id="@+id/humanErrorsTeleop"
        android:layout_width="238dp"
        android:layout_height="0dp"
        android:text="# of Human Errors:"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.602"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/addHumanErrors"
        app:layout_constraintTop_toTopOf="@+id/addHumanErrors" />

    <TextView
        android:id="@+id/foulsTeleop"
        android:layout_width="217dp"
        android:layout_height="0dp"
        android:text="# of Fouls:"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/subtractFoulsTeleop"
        android:layout_marginTop="5dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="5dp"
        app:layout_constraintTop_toTopOf="@+id/subtractFoulsTeleop"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/roboErrorsTeleop"
        android:layout_width="225dp"
        android:layout_height="54dp"
        android:text="# of Robot Errors:"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/humanErrorsTeleop"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="17dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/techFoulsTeleop"
        android:layout_width="206dp"
        android:layout_height="47dp"
        android:text="# of Tech Fouls:"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageButton
        android:id="@+id/addTechFoulsTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription=""
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints,RtlHardcoded"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.403"
        app:layout_constraintLeft_toLeftOf="@+id/addHumanErrors" />

    <ImageButton
        android:id="@+id/addHumanErrors"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addRoboErrorsTeleop"
        android:layout_marginBottom="17dp"
        app:layout_constraintLeft_toLeftOf="@+id/addRoboErrorsTeleop" />

    <ImageButton
        android:id="@+id/addRoboErrorsTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="6dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="101dp" />

    <ImageButton
        android:id="@+id/addFoulsTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints,RtlHardcoded"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addTechFoulsTeleop"
        app:layout_constraintRight_toRightOf="@+id/addTechFoulsTeleop"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/addTechFoulsTeleop" />

    <ImageButton
        android:id="@+id/subtractFoulsTeleop"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginEnd="2dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        app:layout_constraintRight_toLeftOf="@+id/foulsTeleop"
        app:layout_constraintTop_toTopOf="@+id/addFoulsTeleop"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageButton
        android:id="@+id/subtractRoboErrorsTeleop"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginBottom="80dp"
        android:layout_marginTop="69dp"
        android:contentDescription=""
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="@+id/goToPostMatch"
        app:layout_constraintRight_toRightOf="@+id/subtractHumanErrors"
        app:layout_constraintTop_toTopOf="@+id/humanErrorsTeleop"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/subtractHumanErrors"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintRight_toRightOf="@+id/subtractTechFoulsTeleop"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintTop_toTopOf="@+id/addHumanErrors" />

    <ImageButton
        android:id="@+id/subtractTechFoulsTeleop"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintTop_toTopOf="@+id/addTechFoulsTeleop"
        app:layout_constraintVertical_bias="0.407"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints,RtlHardcoded"
        app:layout_constraintRight_toRightOf="@+id/subtractFoulsTeleop" />

    <Switch
        android:id="@+id/climbYesNo"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_marginStart="37dp"
        android:text="Climb?"
        android:thumbTint="@android:color/darker_gray"
        android:trackTint="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView" />

</android.support.constraint.ConstraintLayout>

