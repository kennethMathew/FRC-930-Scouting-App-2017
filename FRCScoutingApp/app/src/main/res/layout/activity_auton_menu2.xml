<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.AutonMenu2"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="46dp"
        app:layout_constraintTop_toBottomOf="@+id/crossBaselineView" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="230dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        android:layout_marginBottom="231dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/crossBaselineView"
        android:layout_marginStart="1dp"
        android:layout_marginTop="46dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="45dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/foulsAuton"
        android:layout_width="242dp"
        android:layout_height="36dp"
        android:text="# of Fouls: 0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintBottom_toBottomOf="@+id/subtractFoulsAuton"
        app:layout_constraintTop_toTopOf="@+id/subtractFoulsAuton" />

    <TextView
        android:id="@+id/techFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:text="# of Tech Fouls: 0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/subtractTechFoulsAuton"
        app:layout_constraintRight_toRightOf="@+id/robotErrorsAuton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.556"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/robotErrorsAuton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="58dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="10dp"
        android:text="# of Robot Errors: 0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintLeft_toLeftOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintRight_toRightOf="@+id/addRoboErrorsAuton"
        app:layout_constraintTop_toTopOf="@+id/subtractRoboErrorsAuton"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/goToTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Teleop"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/addRoboErrorsAuton"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:layout_marginRight="16dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/subtractRoboErrorsAuton"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/subtractRoboErrorsAuton" />

    <ImageButton
        android:id="@+id/subtractRoboErrorsAuton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="124dp"
        android:layout_marginEnd="31dp"
        android:layout_marginStart="31dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/goToTeleop"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageButton
        android:id="@+id/addFoulsAuton"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:layout_marginRight="16dp"
        android:contentDescription=""
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/subtractFoulsAuton"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/subtractFoulsAuton" />

    <ImageButton
        android:id="@+id/subtractFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="@+id/subtractTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/subtractTechFoulsAuton"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/techFoulsAuton"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="52dp"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/addTechFoulsAuton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.555"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/subtractTechFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginBottom="82dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toTopOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintRight_toRightOf="@+id/subtractRoboErrorsAuton"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <CheckBox
        android:id="@+id/crossBaseline"
        android:layout_width="92dp"
        android:layout_height="43dp"
        android:checked="true"
        android:text="Checked\n=Yes"
        app:buttonTint="@android:color/holo_blue_light"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:layout_marginEnd="42dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/crossBaselineView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Cross Baseline"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="98dp"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
