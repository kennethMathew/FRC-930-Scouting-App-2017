<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.AutonMenu2"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/foulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="46dp"
        app:layout_constraintTop_toBottomOf="@+id/crossBaselineView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/subtractRoboErrorsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/goToTeleop"
        app:layout_constraintRight_toRightOf="@+id/subtractTechFoulsAuton"
        android:layout_marginTop="84dp"
        app:layout_constraintTop_toBottomOf="@+id/numFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="85dp"
        app:layout_constraintLeft_toLeftOf="@+id/subtractTechFoulsAuton" />

    <Button
        android:id="@+id/mainMenuAuton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="Main Menu"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Teleop"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/addRoboErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/goToTeleop"
        app:layout_constraintRight_toRightOf="@+id/addTechFoulsAuton"
        android:layout_marginTop="135dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="133dp"
        app:layout_constraintLeft_toLeftOf="@+id/addTechFoulsAuton"
        app:layout_constraintTop_toTopOf="@+id/numFoulsAuton" />

    <TextView
        android:id="@+id/techFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Tech Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/subtractTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toRightOf="@+id/subtractTechFoulsAuton" />

    <ImageButton
        android:id="@+id/addFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        android:contentDescription=""
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addTechFoulsAuton"
        app:layout_constraintRight_toRightOf="@+id/addTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="22dp"
        app:layout_constraintLeft_toLeftOf="@+id/addTechFoulsAuton" />

    <ImageButton
        android:id="@+id/subtractFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractTechFoulsAuton"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="@+id/backAutonMenu2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="24dp"
        app:layout_constraintLeft_toLeftOf="@+id/crossBaselineView" />

    <TextView
        android:id="@+id/numRoboErrorsAuton"
        android:layout_width="36dp"
        android:layout_height="46dp"
        android:text="0"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/goToTeleop"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintRight_toRightOf="@+id/addRoboErrorsAuton"
        android:layout_marginTop="81dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="81dp"
        app:layout_constraintTop_toTopOf="@+id/addTechFoulsAuton"
        app:layout_constraintLeft_toRightOf="@+id/crossBaselineView" />

    <CheckBox
        android:id="@+id/crossBaseline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="Checked =Yes"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/mainMenuAuton2" />

    <TextView
        android:id="@+id/crossBaselineView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cross Baseline"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/crossBaseline" />

    <TextView
        android:id="@+id/numFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:text="0"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/foulsAuton"
        android:layout_marginEnd="20dp"
        app:layout_constraintRight_toRightOf="@+id/numTechFoulsAuton"
        app:layout_constraintTop_toBottomOf="@+id/foulsAuton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/foulsAuton" />

    <ImageButton
        android:id="@+id/subtractTechFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintRight_toRightOf="@+id/subtractFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="@+id/subtractFoulsAuton" />

    <TextView
        android:id="@+id/numTechFoulsAuton"
        android:layout_width="33dp"
        android:layout_height="45dp"
        android:text="0"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/addRoboErrorsAuton"
        android:layout_marginEnd="13dp"
        app:layout_constraintRight_toRightOf="@+id/numRoboErrorsAuton"
        android:layout_marginTop="71dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="71dp"
        app:layout_constraintTop_toTopOf="@+id/numFoulsAuton"
        app:layout_constraintLeft_toRightOf="@+id/techFoulsAuton" />

    <ImageButton
        android:id="@+id/addTechFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addRoboErrorsAuton"
        android:layout_marginEnd="11dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="19dp" />

    <TextView
        android:id="@+id/robotErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Robot Errors:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="9dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintLeft_toRightOf="@+id/subtractRoboErrorsAuton" />

    <Button
        android:id="@+id/backAutonMenu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded" />

</android.support.constraint.ConstraintLayout>
