<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.AutonMenu2"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/foulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/numFoulsAuton"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="18dp"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toTopOf="@+id/subtractFoulsAuton" />

    <ImageButton
        android:id="@+id/subtractRoboErrorsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/subtractTechFoulsAuton"
        android:layout_marginTop="143dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="143dp"
        app:layout_constraintLeft_toLeftOf="@+id/subtractTechFoulsAuton"
        app:layout_constraintTop_toTopOf="@+id/addFoulsAuton" />

    <Button
        android:id="@+id/goToTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Teleop"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/addRoboErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/addTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="140dp"
        app:layout_constraintLeft_toLeftOf="@+id/addTechFoulsAuton" />

    <TextView
        android:id="@+id/techFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Tech Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/subtractTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toRightOf="@+id/subtractTechFoulsAuton" />

    <ImageButton
        android:id="@+id/addFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription=""
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addTechFoulsAuton"
        app:layout_constraintRight_toRightOf="@+id/addTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="22dp"
        app:layout_constraintLeft_toLeftOf="@+id/addTechFoulsAuton" />

    <ImageButton
        android:id="@+id/subtractFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:layout_marginEnd="29dp"
        android:layout_marginStart="31dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintRight_toLeftOf="@+id/foulsAuton"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractTechFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="22dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/numRoboErrorsAuton"
        android:layout_width="33dp"
        android:layout_height="46dp"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/goToTeleop"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/numTechFoulsAuton"
        app:layout_constraintRight_toRightOf="@+id/numTechFoulsAuton"
        app:layout_constraintTop_toTopOf="@+id/addTechFoulsAuton"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <CheckBox
        android:id="@+id/crossBaseline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:checked="true"
        android:text="Checked\n=Yes"
        app:buttonTint="@android:color/holo_blue_light"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/crossBaselineView"
        app:layout_constraintLeft_toRightOf="@+id/crossBaselineView" />

    <TextView
        android:id="@+id/crossBaselineView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cross Baseline"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/foulsAuton"
        android:layout_marginBottom="77dp"
        android:layout_marginLeft="157dp" />

    <TextView
        android:id="@+id/numFoulsAuton"
        android:layout_width="33dp"
        android:layout_height="51dp"
        android:text="0"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginEnd="8dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintRight_toRightOf="@+id/goToTeleop"
        app:layout_constraintBaseline_toBaselineOf="@+id/foulsAuton"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/crossBaseline" />

    <ImageButton
        android:id="@+id/subtractTechFoulsAuton"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractRoboErrorsAuton"
        app:layout_constraintRight_toRightOf="@+id/subtractFoulsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="@+id/subtractFoulsAuton" />

    <TextView
        android:id="@+id/numTechFoulsAuton"
        android:layout_width="31dp"
        android:layout_height="48dp"
        android:text="0"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/techFoulsAuton"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintRight_toRightOf="@+id/numFoulsAuton"
        app:layout_constraintLeft_toRightOf="@+id/numFoulsAuton" />

    <ImageButton
        android:id="@+id/addTechFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addRoboErrorsAuton"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="19dp" />

    <TextView
        android:id="@+id/robotErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Robot Errors:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginEnd="3dp"
        app:layout_constraintRight_toLeftOf="@+id/numRoboErrorsAuton"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/numRoboErrorsAuton"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="3dp" />

</android.support.constraint.ConstraintLayout>
