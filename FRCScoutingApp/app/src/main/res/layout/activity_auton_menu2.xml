<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.AutonMenu2">

    <Button
        android:id="@+id/mainMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:text="Main Menu"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToTeleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:text="Continue To Teleop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/backAutonMenu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/crossBaseline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cross Baseline"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="96dp" />

    <ToggleButton
        android:id="@+id/crossBaseline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        android:textOff="Yes"
        android:textOn="No"
        android:onClick="Change"
        tools:layout_editor_absoluteX="266dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/foulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="219dp" />

    <TextView
        android:id="@+id/techFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Tech Fouls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="289dp" />

    <TextView
        android:id="@+id/robotErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Robot Errors:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="367dp" />

    <ImageButton
        android:id="@+id/addTechFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="277dp" />

    <ImageButton
        android:id="@+id/addRoboErrorsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="353dp" />

    <ImageButton
        android:id="@+id/addFoulsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="209dp" />

    <ImageButton
        android:id="@+id/subtractTechFoulsAuton"
        android:layout_width="57dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="277dp" />

    <ImageButton
        android:id="@+id/subtractRoboErrorsAuton"
        android:layout_width="57dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="353dp" />

    <ImageButton
        android:id="@+id/subtractFoulsAuton"
        android:layout_width="57dp"
        android:layout_height="51dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="209dp" />

    <TextView
        android:id="@+id/numFoulsAuton"
        android:layout_width="34dp"
        android:layout_height="52dp"
        android:text="0"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="244dp"
        tools:layout_editor_absoluteY="219dp" />

    <TextView
        android:id="@+id/numTechFoulsAuton"
        android:layout_width="33dp"
        android:layout_height="45dp"
        android:text="0"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="266dp"
        tools:layout_editor_absoluteY="289dp" />

    <TextView
        android:id="@+id/numRoboErrorsAuton"
        android:layout_width="36dp"
        android:layout_height="46dp"
        android:text="0"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="367dp" />

</android.support.constraint.ConstraintLayout>
