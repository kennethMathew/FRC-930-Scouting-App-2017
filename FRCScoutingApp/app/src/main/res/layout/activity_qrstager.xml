<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.QRStager"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/mainMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="16dp"
        android:text="Main Menu"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/backQRStager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/img_result_qr"
        android:layout_width="0dp"
        android:layout_height="256dp"
        android:background="@android:color/white"
        app:srcCompat="@mipmap/ic_launcher"
        tools:ignore="ContentDescription"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="64dp"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/mainMenu"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.164" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="23dp"
        app:layout_constraintRight_toRightOf="@+id/imageView35"
        android:layout_marginBottom="57dp" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/img_result_qr"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/mainMenu"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/genQRCode"
        android:layout_width="206dp"
        android:layout_height="70dp"
        android:layout_marginBottom="33dp"
        android:text="Generate QR Code"
        app:layout_constraintBottom_toTopOf="@+id/imageView30"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="DuplicateIds,HardcodedText"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />
</android.support.constraint.ConstraintLayout>
