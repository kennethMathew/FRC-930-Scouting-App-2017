<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anew.frcscoutingapp.AutonMenu"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <Button
        android:id="@+id/goToAuton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Auton 2"
        tools:ignore="HardcodedText"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/gears"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gears"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="152dp"
        app:layout_constraintTop_toBottomOf="@+id/ballGroupAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="150dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/balls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Balls"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="112dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/accuracy"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Accuracy"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="255dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="255dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/subtractBallsAuton"
        android:layout_width="51dp"
        android:layout_height="53dp"
        android:layout_marginBottom="78dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="78dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toTopOf="@+id/gearGroupAuton"
        app:layout_constraintLeft_toLeftOf="@+id/balls"
        app:layout_constraintTop_toTopOf="@+id/balls"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Balls:"
        android:textSize="24sp"
        android:layout_marginStart="5dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="11dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/subtractBallsAuton"
        app:layout_constraintLeft_toRightOf="@+id/subtractBallsAuton"
        tools:ignore="HardcodedText" />

    <RadioGroup
        android:id="@+id/gearGroupAuton"
        android:layout_width="133dp"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/goToAuton2"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="79dp"
        app:layout_constraintTop_toBottomOf="@+id/addBallsAuton"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="76dp"
        app:layout_constraintLeft_toLeftOf="parent">

        <RadioButton
            android:id="@+id/noAttemptGearsAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="No Attempt"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/notScoredGearsAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Not Scored"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/scoredGearsAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Scored"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/ballGroupAuton"
        android:layout_width="126dp"
        android:layout_height="111dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/balls"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/balls">

        <RadioButton
            android:id="@+id/noAttemptBallsAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="No Attempt"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/lowGoalAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Low"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/highGoalAuton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="High"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />
    </RadioGroup>

    <ImageButton
        android:id="@+id/addBallsAuton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:layout_marginTop="22dp"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ballGroupAuton"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/numBallsAuton"
        android:layout_width="74dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:layout_marginEnd="13dp"
        app:layout_constraintRight_toLeftOf="@+id/addBallsAuton"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/addBallsAuton"
        android:layout_marginBottom="2dp"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        android:layout_marginRight="13dp"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText,RtlHardcoded" />

</android.support.constraint.ConstraintLayout>
