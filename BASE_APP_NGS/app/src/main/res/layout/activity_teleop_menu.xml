<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.team930.bears.base_app_ngs.TeleopMenu"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        app:layout_constraintRight_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/balls"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="6dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/gears"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="@+id/imageView29"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/addBallsTeleop"
        android:layout_marginBottom="12dp" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/balls"
        android:layout_marginTop="41dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="53dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="434dp"
        android:layout_height="7dp"
        app:srcCompat="@color/zxing_custom_viewfinder_laser"
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/goToTeleop2"
        android:layout_marginEnd="1dp"
        app:layout_constraintRight_toRightOf="@+id/imageView18"
        android:layout_marginTop="94dp"
        android:layout_marginBottom="96dp"
        app:layout_constraintTop_toTopOf="@+id/addGearsTeleop" />

    <Button
        android:id="@+id/goToTeleop2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue To Teleop 2"
        tools:ignore="HardcodedText"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/balls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Balls"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractBallsTeleop"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="61dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gears"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gears"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractGearsTeleop"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/ballsTeleop"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:text="# of Balls:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageView18"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintRight_toRightOf="@+id/addBallsTeleop"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="17dp"
        app:layout_constraintTop_toTopOf="@+id/subtractBallsTeleop"
        app:layout_constraintLeft_toRightOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/gearsTeleop"
        android:layout_width="168dp"
        android:layout_height="0dp"
        android:text="# of Gears:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@android:color/darker_gray"
        android:textSize="24sp"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/subtractGearsTeleop"
        android:layout_marginTop="2dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="2dp"
        app:layout_constraintTop_toTopOf="@+id/subtractGearsTeleop"
        app:layout_constraintLeft_toRightOf="@+id/subtractGearsTeleop"
        android:layout_marginLeft="6dp"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="6dp" />

    <ImageButton
        android:id="@+id/addGearsTeleop"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintRight_toRightOf="@+id/addBallsTeleop"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="153dp" />

    <ImageButton
        android:id="@+id/subtractGearsTeleop"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintLeft_toLeftOf="@+id/subtractBallsTeleop"
        app:layout_constraintBottom_toBottomOf="@+id/addGearsTeleop" />

    <ImageButton
        android:id="@+id/addBallsTeleop"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/addGearsTeleop"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="76dp" />

    <ImageButton
        android:id="@+id/subtractBallsTeleop"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:tint="@android:color/holo_blue_dark"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:ignore="ContentDescription,MissingConstraints"
        app:layout_constraintTop_toTopOf="@+id/addBallsTeleop"
        app:layout_constraintRight_toLeftOf="@+id/ballsTeleop"
        tools:layout_constraintRight_creator="1" />

    <RadioGroup
        android:id="@+id/ballGroupTeleop"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        tools:ignore="MissingConstraints,RtlHardcoded"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/subtractBallsTeleop"
        android:layout_marginStart="15dp"
        android:layout_marginTop="7dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintTop_toTopOf="@+id/imageView16"
        app:layout_constraintLeft_toRightOf="@+id/subtractBallsTeleop">

        <RadioButton
            android:id="@+id/noAttemptBallsTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="No Attempt"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/lowGoalTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Low"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />

        <RadioButton
            android:id="@+id/highGoalTeleop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="High"
            app:buttonTint="@android:color/holo_blue_dark"
            tools:ignore="HardcodedText,MissingPrefix" />
    </RadioGroup>

</android.support.constraint.ConstraintLayout>

